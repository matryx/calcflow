errcatch(expintegral_ei(0.0b0));
[];

expintegral_li(0);
0;
expintegral_li(0.0);
0.0;
expintegral_li(0.0b0);
0.0b0;

errcatch(expintegral_li(1));
[];
errcatch(expintegral_li(1.0));
[];
errcatch(expintegral_li(1.0b0));
[];

expintegral_si(0);
0;
expintegral_si(0.0);
0.0;
expintegral_si(0.0b0);
0.0b0;

expintegral_shi(0);
0;
expintegral_shi(0.0);
