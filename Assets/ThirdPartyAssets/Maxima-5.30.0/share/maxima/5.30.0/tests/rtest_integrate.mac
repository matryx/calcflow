/* A collection of about 730 integrals with power functions.
   The integrals are taken from functions.wolfram.com

   OK   - The integral is solved by Maxima
   NEW  - Solution after extension of the integrator (rev. 1.34)
   NOUN - No solution a noun form is returned.  
*/

(kill(all),
  declare(z,complex,n,integer),
  assume(n>0),
  assume(notequal(v,0)),
  domain:complex,
  radexpand:true,
  
  /* This function checks if we get back the integrand.
   *
   * integral is the result of an integration, 
   * var the integration variable, and
   * simplifier a simplifying function. 
   */
  diffcheck(integral,var,simplifier):=
     if op(integral)=nounify(integrate) then
        integral
     else 
        simplifier(diff(integral,var)),
        
  factor_radcan(expr):=factor(radcan(expr)),
  done);
done;

/* ----- Involving only one direct function --------------------------------- */

/* Problem 2: OK */
integrate(a^(c*z),z);
a^(c*z)/(log(a)*c);

/* Problem 3: OK */
integrate(a^z,z);
a^z/log(a);

/* Problem 4: OK */
integrate(%e^(a*z+b),z);
%e^(a*z+b)/a;

/* Problem 5: OK */
integrate(%e^(a*z),z);
%e^(a*z)/a;

/* Problem 6: OK */
integrate(%e^z,z);
%e^z;

/* ----- Power arguments and base a ----------------------------------------- */

/* Problem 7: NEW Type 1 */
integrate(a^(b*z^r),z);
z*gamma_incomplete(1/r,-b*z^r*log(a))*(-b*z^r*log(a))^(-1/r)/-r;

/* Problem 9: OK */
assume(a>1,b>0);
[a>1,b>0];
integrate(a^(b*z^2),z),erf_representation:erfi,trigsign:true;
-sqrt(%pi)*%i*erfi(%i*sqrt(-log(a)*b)*z)/(2*sqrt(-log(a)*b))$
forget(a>1,b>0);
[a>1,b>0];

/* Problem 11: OK */
integrate(a^(b*sqrt(z)),z),radcan,factor;
2*(log(a)*b*sqrt(z)-1)*a^(b*sqrt(z))/(log(a)^2*b^2);

/* Problem 12: NEW Type 1 */
integrate(a^(b*(z^r)^p),z);
z*gamma_incomplete(1/(p*r),-log(a)*b*(z^r)^p)
  /(-p*r*(-b*log(a)*(z^r)^p)^(1/(p*r)));

/* Problem 13: Expected failure
 * (z^r)^(1/r) simplifies to z which is not correct in general.
 * rischint does not respect the complex character of z.
 * The result of rischint is only correct for z>0.
 */
integrate(a^(b*(z^r)^(1/r)),z), domain:real, radcan;
a^(b*z)/(log(a)*b)$

/* Problem 14: Expected failure 
 * sqrt(z^2) simplifies to z, which is not correct in general.
 * rischint does not respect the complex character of z.
 * The result of rischint is only correct for z>0.
 */
integrate(a^(b*sqrt(z^2)),z), radcan;
a^(b*z)/(log(a)*b)$

/* ----- Power arguments and base %e ---------------------------------------- */

/* Problem 15: NEW Type 1 */
integrate(%e^(a*z^r),z);
-z*gamma_incomplete(1/r,-a*z^r)/(r*(-a*z^r)^(1/r));

/* Problem 17: OK */
assume(a>0);
[a>0];
integrate(%e^(a*z^2),z),erf_representation:erfi;
-sqrt(%pi)*%i*erfi(%i*sqrt(-a)*z)/(2*sqrt(-a))$
forget(a>0);
[a>0];

/* Problem 19: OK */
integrate(%e^(a*sqrt(z)),z);
2*(a*sqrt(z)-1)*%e^(a*sqrt(z))/a^2;

/* Problem 20: NEW Type 1 */
integrate(%e^(a*(z^r)^p),z);
-z*gamma_incomplete(1/(p*r),-a*(z^r)^p)/(p*r*(-a*(z^r)^p)^(1/(p*r)));

/* Problem 21: Expected failure 
 * See problem 13
 */
integrate(%e^(a*(z^r)^(1/r)),z), domain:real;
%e^(a*z)/a$

/* Problem 22: Expected failure 
 * See problem 14
 */
integrate(%e^(a*sqrt(z^2)),z);
%e^(a*z)/a$

/* ----- Involving z^(v-1), arguments b*z and base a ------------------------ */

/* Problem 23: NEW Type 2 */
integrate(z^(v-1)*a^(b*z),z);
-z^v*gamma_incomplete(v,-log(a)*b*z)/(-b*z*log(a))^v;

/* Problem 24: NEW Type 2 */
integrate(a^(b*z)/z,z),gamma_expand:true,logcontract;
expintegral_ei(b*z*log(a));

/* Problem 25: NEW Type 2 */
integrate(z^(v-1)*a^z,z);
-gamma_incomplete(v,-log(a)*z)*z^v/(-log(a)*z)^v;

/* Problem 26: NEW Type 2 */
integrate(a^z/z,z),gamma_expand:true,logcontract;
expintegral_ei(z*log(a));


/* ----- Involving z^(v-1), arguments a*z and base %e ----------------------- */

/* Problem 27: NEW Type 2 */
integrate(z^(v-1)*%e^(a*z),z);
-gamma_incomplete(v,-a*z)*z^v/(-a*z)^v;

/* Problem 28: NEW Type 2 */
integrate(z^(v-1)*%e^z,z);
-gamma_incomplete(v,-z)*z^v/(-z)^v;

/* Problem 29: NEW Type 2 */
integrate(z^n*%e^(a*z),z),factor;
a^(-n-1)*(-1)^n*gamma_incomplete(n+1,-a*z);

/* Problem 30: New Type 2 */
integrate(%e^(a*z)/z^n,z),factor;
a^(n-1)*(-1)^n*gamma_incomplete(1-n,-a*z);

/* Problem 31: OK */
integrate(z*%e^(a*z),z);
(a*z-1)*%e^(a*z)/a^2;

/* Problem 32: Expected failure 
 * Simplifies sqrt(z^2) -> z, which is correct only for z>0.
 * The problem is in rischint.
 */
integrate(sqrt(z^2)*%e^(a*z),z);
(a*z-1)*%e^(a*z)/a^2$

/* Problem 33: OK */
integrate(z^2*%e^(a*z),z);
(a^2*z^2-2*a*z+2)*%e^(a*z)/a^3;

/* Problem 34: OK */
integrate(z^3*%e^(a*z),z);
(a^3*z^3-3*a^2*z^2+6*a*z-6)*%e^(a*z)/a^4;

/* Problem 35: OK */
integrate(z^4*%e^(a*z),z);
(a^4*z^4-4*a^3*z^3+12*a^2*z^2-24*a*z+24)*%e^(a*z)/a^5;

/* Problem 36: OK */
integrate(z^5*%e^(a*z),z);
(a^5*z^5-5*a^4*z^4+20*a^3*z^3-60*a^2*z^2+120*a*z-120)*%e^(a*z)/a^6;

/* Problem 37: OK */
integrate(z^6*%e^(a*z),z);
(a^6*z^6-6*a^5*z^5+30*a^4*z^4-120*a^3*z^3+360*a^2*z^2-720*a*z+720)
        *%e^(a*z)
        /a^7;

/* Problem 38: OK */
integrate(z^7*%e^(a*z),z);
(a^7*z^7-7*a^6*z^6+42*a^5*z^5-210*a^4*z^4+840*a^3*z^3-2520*a^2*z^2
               +5040*a*z-5040)
        *%e^(a*z)
        /a^8;

/* Problem 39: OK */
integrate(z^8*%e^(a*z),z);
(a^8*z^8-8*a^7*z^7+56*a^6*z^6-336*a^5*z^5+1680*a^4*z^4-6720*a^3*z^3
               +20160*a^2*z^2-40320*a*z+40320)
        *%e^(a*z)
        /a^9;

/* Problem 40: NEW Type 2 */
integrate(%e^(a*z)/z,z),gamma_expand:true,logcontract;
expintegral_ei(a*z);

/* Problem 41: NEW Type 2 */
integrate(%e^(a*z)/z^2,z),gamma_expand:true,logcontract,ratsimp;
(a*z*expintegral_ei(a*z)-%e^(a*z))/z;

/* Problem 42: NEW Type 2 */
integrate(%e^(a*z)/z^3,z),gamma_expand:true,logcontract,ratsimp;
(a^2*z^2*expintegral_ei(a*z)+(-a*z-1)*%e^(a*z))/(2*z^2);

/* Problem 43: NEW Type 2 */
integrate(%e^(a*z)/z^4,z),gamma_expand:true,logcontract,ratsimp;
(a^3*z^3*expintegral_ei(a*z)+(-a^2*z^2-a*z-2)*%e^(a*z))/(6*z^3);

/* Problem 44: NEW Type 2 */
integrate(%e^(a*z)/z^5,z),gamma_expand:true,logcontract,ratsimp;
(a^4*z^4*expintegral_ei(a*z)+(-a^3*z^3-a^2*z^2-2*a*z-6)*%e^(a*z))/(24*z^4);

/* Problem 45: NEW Type 2 (OK)
 * The following gives sqrt(z)/sqrt(-z) and not sqrt(-z)/sqrt(z),
 * but the phase is correct.
 */
integrate(z^(1/2+n)*%e^(a*z),z),ratsimp;
a^(-n-1)*(-1)^n*gamma_incomplete((2*n+3)/2,-a*z)*sqrt(z)/sqrt(-a*z);

/* Problem 47: OK */
assume(a>0);
[a>0];
integrate(sqrt(z)*%e^(a*z),z),erf_representation:erfi,expand;
sqrt(z)*%e^(a*z)/a+sqrt(%pi)*%i*erfi(%i*sqrt(-a)*sqrt(z))/(2*sqrt(-a)*a)$
forget(a>0);
[a>0];

/* Problem 50: OK */
assume(a>0);
[a>0];
multthru(integrate(z^(3/2)*%e^(a*z),z)),erf_representation:erfi;
(2*a*z^(3/2)-3*sqrt(z))*%e^(a*z)/(2*a^2)
 -3*sqrt(%pi)*%i*erfi(%i*sqrt(-a)*sqrt(z))/(4*sqrt(-a)*a^2)$
forget(a>0);
[a>0];

/* Problem 53: OK */
assume(a>0);
[a>0];
multthru(integrate(z^(5/2)*%e^(a*z),z)),erf_representation:erfi;
(4*a^2*z^(5/2)-10*a*z^(3/2)+15*sqrt(z))*%e^(a*z)/(4*a^3)
 +15*sqrt(%pi)*%i*erfi(%i*sqrt(-a)*sqrt(z))/(8*sqrt(-a)*a^3);
forget(a>0);
[a>0];

/* Problem 56: OK */
assume(a>0);
[a>0];
multthru(integrate(z^(7/2)*%e^(a*z),z)),erf_representation:erfi;
(8*a^3*z^(7/2)-28*a^2*z^(5/2)+70*a*z^(3/2)-105*sqrt(z))*%e^(a*z)/(8*a^4)
 -105*sqrt(%pi)*%i*erfi(%i*sqrt(-a)*sqrt(z))/(16*sqrt(-a)*a^4);
forget(a>0);
[a>0];

/* Problem 59: OK */
assume(a>0);
[a>0];
multthru(integrate(z^(9/2)*%e^(a*z),z)),erf_representation:erfi;
(16*a^4*z^(9/2)-72*a^3*z^(7/2)+252*a^2*z^(5/2)-630*a*z^(3/2)+945*sqrt(z))
 *%e^(a*z)
 /(16*a^5)
 +945*sqrt(%pi)*%i*erfi(%i*sqrt(-a)*sqrt(z))/(32*sqrt(-a)*a^5)$
forget(a>0);
[a>0];

/* Problem 62: OK */
assume(a>0);
[a>0];
integrate(%e^(a*z)/sqrt(z),z),erf_representation:erfi;
-sqrt(%pi)*%i*erfi(%i*sqrt(-a)*sqrt(z))/sqrt(-a)$
forget(a>0);
[a>0];

/* Problem 64: NEW Type 2 (OK)
 * We have a problem to simplify this integral to an erfi representation, 
 * because of expression like sqrt(-z), which do not simplify to %i*sqrt(z).
 * We give the results in term of the erfc function.
 * The same for No. 48, 49, 50, 51 
 */
multthru(integrate(%e^(a*z)/z^(3/2),z)),gamma_expand:true;
2*sqrt(%pi)*sqrt(-a*z)*erfc(sqrt(-a*z))/sqrt(z)-2*%e^(a*z)/sqrt(z);

/* Problem 65: NEW Type 2 (OK)
 * Maxima simplifies z/sqrt(z^3) -> 1/sqrt(z). Therefore, we get the same
 * result for Problem 64 and 65.
 * Because of revision 1.125 of simp.lisp Maxima no longer simplifies
 * z/sqrt(z^3) -> 1/sqrt(z). We get a noun form.
 */
multthru(integrate(%e^(a*z)/sqrt(z^3),z)),gamma_expand:true, domain:real;
2*sqrt(%pi)*sqrt(-a*z)*erfc(sqrt(-a*z))/sqrt(z)-2*%e^(a*z)/sqrt(z)$
/*'integrate(%e^(a*z)/sqrt(z^3),z);*/

/* Problem 66: NEW Type 2 (OK) */
multthru(integrate(%e^(a*z)/z^(5/2),z)),gamma_expand:true;
(-4*a*z/3-2/3)*%e^(a*z)/z^(3/2)-4*sqrt(%pi)*(-a*z)^(3/2)*erfc(sqrt(-a*z))
                                /(3*z^(3/2));

/* Problem 67: NEW Type 2 (OK) */
multthru(integrate(%e^(a*z)/z^(7/2),z)),gamma_expand:true;
8*sqrt(%pi)*(-a*z)^(5/2)*erfc(sqrt(-a*z))/(15*z^(5/2))
 +(-8*a^2*z^2/15-4*a*z/15-2/5)*%e^(a*z)/z^(5/2);

/* Problem 68: NEW Type 2 (OK) */
multthru(integrate(%e^(a*z)/z^(9/2),z)),gamma_expand:true;
(-16*a^3*z^3/105-8*a^2*z^2/105-4*a*z/35-2/7)*%e^(a*z)/z^(7/2)
 -16*sqrt(%pi)*(-a*z)^(7/2)*erfc(sqrt(-a*z))/(105*z^(7/2));

/* ----- Involving z^(v-1), arguments b*z+d and base a ---------------------- */

/* Problem 69: NEW Type 2 */
integrate(z^(v-1)*a^(b*z+d),z);
-a^d*gamma_incomplete(v,-log(a)*b*z)*z^v/(-log(a)*b*z)^v$

/* Problem 70: NEW 2 */
integrate(a^(b*z+d)/z,z),gamma_expand:true,logcontract;
a^d*expintegral_ei(b*z*log(a));

/* ----- Involving z^(v-1), arguments b*z+d and base %e --------------------- */

/* Problem 71: NEW 2 */
diffcheck(integrate(z^(v-1)*%e^(a*z+b),z),z,factor);
z^(v-1)*%e^(a*z+b);

/* Problem 72: NEW 2 */
integrate(z^n*%e^(a*z+b),z),factor;
a^(-n-1)*%e^b*(-1)^n*gamma_incomplete(n+1,-a*z);

/* Problem 73: OK */
integrate(z*%e^(a*z+b),z),factor;
(a*z-1)*%e^(a*z+b)/a^2;

/* Problem 74: NEW 2 */
integrate(%e^(a*z+b)/z,z),gamma_expand:true,logcontract;
%e^b*expintegral_ei(a*z);

/* Problem 76: OK */
assume(a>0);
[a>0];
integrate(%e^(a*z+b)/sqrt(z),z),erf_representation:erfi;
-sqrt(%pi)*%i*%e^b*erfi(%i*sqrt(-a)*sqrt(z))/sqrt(-a)$
forget(a>0);
[a>0];

/* ----- Involving z^(v-1), arguments b*z^r and base a ---------------------- */

/* Problem 77: NEW 2 */
integrate(z^(v-1)*a^(b*z^r),z);
-gamma_incomplete(v/r,-log(a)*b*z^r)*z^v/(r*(-log(a)*b*z^r)^(v/r))$

/* Problem 79: NEW 2 */
integrate(a^(b*z^r)/z,z),gamma_expand:true,logcontract;
expintegral_ei(log(a)*b*z^r)/r;

/* ----- Involving z^(v-1), arguments a*z^r and base %e --------------------- */

/* Problem 80: NEW 2 */
integrate(z^(v-1)*%e^(a*z^r),z);
-gamma_incomplete(v/r,-a*z^r)*z^v/(r*(-a*z^r)^(v/r))$

/* Problem 81: NEW 2 */
integrate(%e^(a*z^r)/z,z),gamma_expand:true,logcontract;
expintegral_ei(a*z^r)/r;

/* Problem 82: NEW 2 */
integrate(z^(2*n)*%e^(a*z^2),z),factor;
-(-1)^n*gamma_incomplete((2*n+1)/2,-a*z^2)*z/(2*a^n*sqrt(-a*z^2))$

/* Problem 83: NEW 2 */
integrate(z^(2*n-1)*%e^(a*z^2),z),factor;
-(-1)^n*gamma_incomplete(n,-a*z^2)/(2*a^n);

/* Problem 84: OK */
integrate(z*%e^(a*z^2),z);
%e^(a*z^2)/(2*a);

/* Problem 86: OK */
assume(a>0);
[a>0];
integrate(z^2*%e^(a*z^2),z),erf_representation:erfi;
sqrt(%pi)*%i*erfi(%i*sqrt(-a)*z)/(4*sqrt(-a)*a)+z*%e^(a*z^2)/(2*a)$
forget(a>0);
[a>0];

/* Problem 88: OK */
integrate(z^3*%e^(a*z^2),z);
(a*z^2-1)*%e^(a*z^2)/(2*a^2);

/* Problem 90: OK */
assume(a>0);
[a>0];
integrate(z^4*%e^(a*z^2),z),erf_representation:erfi;
(2*a*z^3-3*z)*%e^(a*z^2)/(4*a^2)-3*sqrt(%pi)*%i*erfi(%i*sqrt(-a)*z)
                                 /(8*sqrt(-a)*a^2)$
forget(a>0);
[a>0];

/* Problem 92: OK */
integrate(z^5*%e^(a*z^2),z);
(a^2*z^4-2*a*z^2+2)*%e^(a*z^2)/(2*a^3);

/* Problem 93: NEW 2 */
integrate(%e^(a*z^2)/z,z),gamma_expand:true,logcontract;
expintegral_ei(a*z^2)/2;

/* Problem 94: NEW 2 */
multthru(integrate(%e^(a*z^2)/z^2,z)), gamma_expand:true;
sqrt(%pi)*sqrt(-a*z^2)*erfc(sqrt(-a*z^2))/z-%e^(a*z^2)/z;

/* Problem 95: NEW 2 */
integrate(%e^(a*z^2)/z^3,z),gamma_expand:true,logcontract,ratsimp;
(a*z^2*expintegral_ei(a*z^2)-%e^(a*z^2))/(2*z^2);

/* Problem 96: NEW 2 */
integrate(%e^(a*z^2)/z^4,z), gamma_expand:true,ratsimp;
(2*sqrt(%pi)*a^2*z^4*erfc(sqrt(-a*z^2))+sqrt(-a*z^2)*(2*a*z^2+1)*%e^(a*z^2))
 /(3*z^3*sqrt(-a*z^2))/-1;

/* Problem 97: NEW 2 */
integrate(%e^(a*z^2)/z^5,z),gamma_expand:true,logcontract;
(2*a^2*z^4*expintegral_ei(a*z^2)+(-2*a*z^2-2)*%e^(a*z^2))/(8*z^4);

/* Problem 98: NEW 2 */
integrate(z^n*%e^(a*sqrt(z)),z);
-2*gamma_incomplete(2*(n+1),-a*sqrt(z))/a^(2*(n+1));

/* Problem 99: OK */
integrate(z*%e^(a*sqrt(z)),z);
2*(a^3*z^(3/2)-3*a^2*z+6*a*sqrt(z)-6)*%e^(a*sqrt(z))/a^4;

/* Problem 100: OK */
integrate(z^2*%e^(a*sqrt(z)),z);
2*(a^5*z^(5/2)-5*a^4*z^2+20*a^3*z^(3/2)-60*a^2*z+120*a*sqrt(z)-120)
        *%e^(a*sqrt(z))
        /a^6;

/* Problem 101: OK */
integrate(z^3*%e^(a*sqrt(z)),z);
2*(a^7*z^(7/2)-7*a^6*z^3+42*a^5*z^(5/2)-210*a^4*z^2+840*a^3*z^(3/2)
                     -2520*a^2*z+5040*a*sqrt(z)-5040)*%e^(a*sqrt(z))
        /a^8;

/* Problem 102: OK */
integrate(z^4*%e^(a*sqrt(z)),z);
2*(a^9*z^(9/2)-9*a^8*z^4+72*a^7*z^(7/2)-504*a^6*z^3+3024*a^5*z^(5/2)
                     -15120*a^4*z^2+60480*a^3*z^(3/2)-181440*a^2*z
                     +362880*a*sqrt(z)-362880)*%e^(a*sqrt(z))
        /a^10;

/* Problem 103: OK */
integrate(z^5*%e^(a*sqrt(z)),z);
2*(a^11*z^(11/2)-11*a^10*z^5+110*a^9*z^(9/2)-990*a^8*z^4
                       +7920*a^7*z^(7/2)-55440*a^6*z^3+332640*a^5*z^(5/2)
                       -1663200*a^4*z^2+6652800*a^3*z^(3/2)-19958400*a^2*z
                       +39916800*a*sqrt(z)-39916800)*%e^(a*sqrt(z))
        /a^12;

/* Problem 104: NEW 2 */
integrate(%e^(a*sqrt(z))/z,z),gamma_expand:true,logcontract;
2*expintegral_ei(a*sqrt(z));

/* Problem 105: NEW 2 */
integrate(%e^(a*sqrt(z))/z^2,z),gamma_expand:true,logcontract;
((2*a*z+2*sqrt(z))*%e^(a*sqrt(z))-2*a^2*expintegral_ei(a*sqrt(z))*z^(3/2))
 /-(2*z^(3/2));

/* Problem 106: NEW 2 */
integrate(%e^(a*sqrt(z))/z^3,z),gamma_expand:true,logcontract;
((2*a^3*z^3+2*a^2*z^(5/2)+4*a*z^2+12*z^(3/2))*%e^(a*sqrt(z))
 -2*a^4*expintegral_ei(a*sqrt(z))*z^(7/2))
 /-(24*z^(7/2));

/* Problem 107: NEW 2 */
integrate(%e^(a*sqrt(z))/z^4,z),gamma_expand:true,logcontract;
((2*a^5*z^5+2*a^4*z^(9/2)+4*a^3*z^4+12*a^2*z^(7/2)+48*a*z^3+240*z^(5/2))
 *%e^(a*sqrt(z))
 -2*a^6*expintegral_ei(a*sqrt(z))*z^(11/2))
 /-(720*z^(11/2));

/* Problem 108: NEW 2 */
integrate(%e^(a*sqrt(z))/z^5,z),gamma_expand:true,logcontract;
((2*a^7*z^7+2*a^6*z^(13/2)+4*a^5*z^6+12*a^4*z^(11/2)+48*a^3*z^5
            +240*a^2*z^(9/2)+1440*a*z^4+10080*z^(7/2))
 *%e^(a*sqrt(z))
 -2*a^8*expintegral_ei(a*sqrt(z))*z^(15/2))
 /-(40320*z^(15/2));

/* ----- Involving z^(v-1), arguments b*z^r and base a ---------------------- */

/* Problem 109: NEW 2 */
diffcheck(integrate(z^(v-1)*a^(d+b*z^r),z),z,radcan);
z^(v-1)*a^(d+b*z^r);

/* Problem 110: NEW 2 */
integrate(a^(d+b*z^r)/z,z),gamma_expand:true,logcontract;
a^d*expintegral_ei(log(a)*b*z^r)/r;

/* ----- Involving z^(v-1), arguments b*z^r and base %e --------------------- */

/* Problem 111: NEW 2 */
diffcheck(integrate(z^(v-1)*%e^(b+a*z^r),z),z,factor);
z^(v-1)*%e^(b+a*z^r);

/* Problem 112: NEW 2 */
integrate(%e^(b+a*z^r)/z,z),gamma_expand:true,logcontract;
%e^b*expintegral_ei(a*z^r)/r;

/* ----- Involving (a*z+b)^-n ----------------------------------------------- */

/* Problem 113: NEW 3 */
integrate(%e^(c*z)/(b+a*z),z),expintexpand:true,logcontract;
%e^-(b*c/a)*expintegral_ei((a*c*z+b*c)/a)/a;

/* Problem 114: NEW 3 */
integrate(%e^(c*z)/(b+a*z)^2,z);
-expintegral_e(2,-c*(a*z+b)/a)*%e^-(b*c/a)/(a*(a*z+b));

/* Problem 115: NEW 3 */
