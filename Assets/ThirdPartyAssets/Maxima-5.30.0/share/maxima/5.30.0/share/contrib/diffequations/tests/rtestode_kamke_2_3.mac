[y=%k2*bessel_y(sqrt(-4*d+b^2-2*b+1)/2,-sqrt(c-a^2)*x)*%e^-(a*x)/x^(b/2-1/2)
  +%k1*bessel_j(sqrt(-4*d+b^2-2*b+1)/2,-sqrt(c-a^2)*x)*%e^-(a*x)/x^(b/2-1/2)];
