[y = bessel_y(-(3/2),-(%i*a*x/2))*%k2*sqrt(-x)*%e^-(b*x+a*x/2)
    +bessel_j(-(3/2),-(%i*a*x/2))*%k1*sqrt(-x)*%e^-(b*x+a*x/2)];
