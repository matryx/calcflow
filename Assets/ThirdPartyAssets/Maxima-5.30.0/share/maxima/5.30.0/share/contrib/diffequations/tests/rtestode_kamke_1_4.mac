/*  301  */ 
(pn_(301),ans:contrib_ode(eqn:(6*x*y^2+x^2)*'diff(y,x,1)-y*(3*y^2-x),y,x));
[x=%c*%e^-((2*x*log(y/sqrt(x))+6*y^2)/(3*x))];
method;
genhom;

