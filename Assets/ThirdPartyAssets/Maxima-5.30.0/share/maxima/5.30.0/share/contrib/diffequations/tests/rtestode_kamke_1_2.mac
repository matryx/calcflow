/* 114 */
assume(x>0,y>0);
[x>0,y>0];
(pn_(114),ans:contrib_ode(eqn:x*'diff(y,x)-x*sqrt(y^2+x^2)-y,y,x));
[x-asinh(y/x)=%c];
