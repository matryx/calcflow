[y = x*sinh(log(x/%c))];
[method,ode_check(eqn,%[1])];
[genhom,0];
forget(x>0);
[x>0];

/* 113 */
(pn_(113),ans:contrib_ode(eqn:x*'diff(y,x)+a*sqrt(y^2+x^2)-y,y,x));
[x=%c*%e^-(x*asinh(y/x)/(a*abs(x)))];
